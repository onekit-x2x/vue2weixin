<!-- ----------------------------------------------  input  ------------------------------------------------------------ -->

 <onekit-p>type=text、password、submit、button:</onekit-p>
<onekit-hr />
<onekit-div>
    <onekit-label for="username">Username:</onekit-label>
    <onekit-input type="text" id="username" name="username"/>
</onekit-div>
<onekit-div>
    <onekit-label for="pass">Password (8 characters minimum):</onekit-label>
    <onekit-input type="password" id="pass" name="password"
           minlength="8" required/>
</onekit-div>
<onekit-input type="submit" value="Sign in"/> 

<onekit-p>type=button:</onekit-p>
<onekit-hr/>
<onekit-input type="button" value="Click me"/>

<onekit-p>type=checkbox:</onekit-p>
<onekit-hr />
<onekit-p>
  <onekit-input type="checkbox" id="cbox1" value="first_checkbox"/>
  <onekit-label for="cbox1">This is the first checkbox</onekit-label>
</onekit-p>
<onekit-p>
  <onekit-input type="checkbox" id="cbox2" value="second_checkbox" checked="checked"/>
  <onekit-label for="cbox2">This is the second checkbox, which is checked</onekit-label>
</onekit-p>

<onekit-p>type=file:</onekit-p>
<onekit-hr />
<onekit-input type="file"
       onekit-id="avatar" name="avatar"
       accept="image/png, image/jpeg"/>

<onekit-p>type=hidden:</onekit-p>
<onekit-hr />
<onekit-input onekit-id="prodId" name="prodId" type="hidden" value="xm234jq"/>

<onekit-p>type=image:</onekit-p>
<onekit-hr />
<onekit-input type="image" name="image" src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1599479802285&di=ca55bdc853b5f1dd47cf8391f7685507&imgtype=0&src=http%3A%2F%2Fa4.att.hudong.com%2F22%2F59%2F19300001325156131228593878903.jpg" width="200"/>



<onekit-p>type=radio、submit:</onekit-p>
<onekit-hr />
<form>
  <onekit-p>Please select your preferred contact method:</onekit-p>
  <onekit-div>
    <onekit-input type="radio" onekit-id="contactChoice1" onekit-class="aaa" onekit-style="bbb"
     name="contact" value="email"/>
    <onekit-label for="contactChoice1">Email</onekit-label>

    <onekit-input type="radio" onekit-id="contactChoice2"
     name="contact" value="phone"/>
    <onekit-label for="contactChoice2">Phone</onekit-label>

    <onekit-input type="radio" onekit-id="contactChoice3"
     name="contact" value="mail"/>
    <onekit-label for="contactChoice3">Mail</onekit-label>
  </onekit-div>
  <onekit-div>
    <onekit-button type="submit">Submit</onekit-button>
  </onekit-div>
</form>

<onekit-p>type=text、submit、reset:</onekit-p>
<onekit-hr />
<form>
    <onekit-div class="controls">

        <onekit-label for="id">User ID:</onekit-label>
        <onekit-input type="text" onekit-id="id" name="id" />

        <onekit-input type="reset" value="Reset"/>
        <onekit-input type="submit" value="Submit"/>

    </onekit-div>
</form> 


