    -webkit-margin-after: 1em;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
    -webkit-padding-start: 40px;
}

.onekit-meter {
    appearance: initial;
    box-sizing: border-box;
    display: inline-block;
    height: 1em;
    width: 5em;
    vertical-align: -0.2em;
    -webkit-user-modify: read-only;
}

.onekit-nobr {
    white-space: nowrap;
}

.onekit-noframes {
    display: none;
}

.onekit-ol {
    display: block;
    list-style-type: decimal;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
    -webkit-padding-start: 40px;
}

.onekit-li {
    display: list-item;
    text-align: match-parent;
}


.onekit-optgroup {
    font-weight: bolder;
    display: block;
}

.onekit-option {
    font-weight: normal;
    display: block;
    white-space: pre;
    min-height: 1.2em;
    padding: 0 2px 1px;
}

.onekit-output {
    display: inline;
}

.onekit-bdi,
.onekit-output {
    unicode-bidi: isolate;
}

.onekit-p {
    display: block;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
}

.onekit-progress {
    appearance: none;
    box-sizing: border-box;
    display: inline-block;
    height: 1em;
    width: 10em;
    vertical-align: -0.2em;
}

.onekit-q::after {
    content: close-quote;
}

.onekit-q::before {
    content: open-quote;
}

.onekit-q {
    display: inline;
}

.onekit-ruby,
.onekit-rt {
    text-indent: 0;
}

.onekit-ruby>.onekit-rp {
    display: none;
}

.onekit-ruby,
.onekit-rt {
    text-indent: 0;
}

.onekit-rt {
    line-height: normal;
    text-emphasis: none;
}

.onekit-ruby>.onekit-rt {
    display: block;
    font-size: 50%;
    text-align: start;
}


.onekit-script {
    display: none;
}



.onekit-select {
    border-radius: 5px;
    -webkit-appearance: menulist;
    box-sizing: border-box;
    align-items: center;
    white-space: pre;
    -webkit-rtl-ordering: logical;
    color: black;
    background-color: white;
    cursor: default;
    border-width: 1px;
    border-style: solid;
    border-color: initial;
    border-image: initial;
}

.onekit-select,
.onekit-input[type="color"][list] {
    background-color: rgb(248, 248, 248);
    border-width: 1px;
    border-style: solid;
    border-color: rgb(166, 166, 166);
    border-image: initial;
}

