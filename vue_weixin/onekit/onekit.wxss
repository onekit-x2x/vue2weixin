.onekit-page {
    height: 100%;
}

.onekit-html {
    -webkit-locale: "en";
}

.onekit-html {
    display: block;
}

.onekit-body {
    display: block;
    margin: 8px;
}

.onekit-div {
    display: block;
}

.onekit-a {
    color: -webkit-link;
    cursor: auto;
    text-decoration: underline;
}

.onekit-abbr[title],
.onekit-acronym[title] {
    text-decoration: underline dotted;

}

.onekit-address {
    display: block;
}

.onekit-i,
.onekit-cite,
.onekit-em,
.onekit-var,
.onekit-address,
.onekit-dfn {
    font-style: italic;
}

.onekit-area {
    display: inline;
    cursor: pointer;
}

.onekit-article,
.onekit-aside,
.onekit-footer,
.onekit-header,
.onekit-hgroup,
.onekit-main,
.onekit-nav,
.onekit-section {
    display: block;
}

.onekit-audio {
    width: 300px;
    height: 32px;
}

.onekit-audio:not([controls]) {
    display: none;
}

.onekit-strong,
.onekit-b {
    font-weight: bold;
}

.onekit-bdi,
.onekit-output {
    unicode-bidi: isolate;
}

.onekit-bdo {
    unicode-bidi: bidi-override;
}

.onekit-big {
    font-size: larger;
}

.onekit-blockquote {
    display: block;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 40px;
    -webkit-margin-end: 40px;
}

.onekit-button {
    appearance: none;
}

.onekit-input,
.onekit-textarea,
.onekit-select,
.onekit-button,
.onekit-meter,
.onekit-progress {
    writing-mode: horizontal-tb;
}

.onekit-input,
.onekit-textarea,
.onekit-select,
.onekit-button {
    text-rendering: auto;
    color: initial;
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0;
    text-shadow: none;
    display: inline-block;
    text-align: start;
    margin: 0;
    font: 11px system-ui;
}

.onekit-input[type="button"],
.onekit-input[type="submit"],
.onekit-input[type="reset"],
.onekit-input[type="file"]::-webkit-file-upload-button,
.onekit-button {
    align-items: flex-start;
    text-align: center;
    cursor: default;
    color: buttontext;
    background-color: buttonface;
    box-sizing: border-box;
    padding: 2px 6px 3px;
    border-width: 2px;
    border-style: outset;
    border-color: buttonface;
    border-image: initial;
}

.onekit-center {
    display: block;
    text-align: center;
}


.onekit-tt,
.onekit-code,
.onekit-kbd,
.onekit-samp {
    font-family: monospace;
}

.onekit-datalist {
    display: none;
}

.onekit-dd {
    display: block;
    -webkit-margin-start: 40px;
}

.onekit-s,
.onekit-strike,
.onekit-del {
    text-decoration: line-through;
}

.onekit-details {
    display: block;
}

.onekit-dialog::backdrop {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.1);
}

.onekit-dialog {
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    width: fit-content;
    height: fit-content;
    color: black;
    margin: auto;
    border-width: initial;
    border-style: solid;
    border-color: initial;
    border-image: initial;
    padding: 1em;
    background: white;
}

.onekit-dialog:not([open]) {
    display: none;
}

.onekit-ul,
.onekit-menu,
.onekit-dir {
    display: block;
    list-style-type: disc;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
    -webkit-padding-start: 40px;
}

.onekit-div {
    display: block;
}

.onekit-dl {
    display: block;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
}

.onekit-dt {
    display: block;
}

.onekit-i,
.onekit-cite,
.onekit-em,
.onekit-var,
.onekit-address,
.onekit-dfn {
    font-style: italic;
}

.onekit-fieldset {
    display: block;
    -webkit-margin-start: 2px;
    -webkit-margin-end: 2px;
    -webkit-padding-before: 0.35em;
    -webkit-padding-start: 0.75em;
    -webkit-padding-end: 0.75em;
    -webkit-padding-after: 0.625em;
    min-width: min-content;
    border-width: 2px;
    border-style: groove;
    border-color: threedface;
    border-image: initial;
}

.onekit-figcaption {
    display: block;
}

.onekit-figure {
    display: block;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 40px;
    -webkit-margin-end: 40px;
}

.onekit-form {
    display: block;
    margin-top: 0;
}

.onekit-h1 {
    display: block;
    font-size: 2em;
    -webkit-margin-before: 0.67em;
    -webkit-margin-after: 0.67em;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
     font-weight: bold;
 }
 .onekit-section .onekit-h1{
    font-size: 1.5em;
    margin-block-start: 0.83em;
    margin-block-end: 0.83em;
 }
.onekit-nav .onekit-h1{
    font-size: 1.5em;
    margin-block-start: 0.83em;
    margin-block-end: 0.83em;
}
.onekit-aside .onekit-h1{
    font-size: 1.5em;
    margin-block-start: 0.83em;
    margin-block-end: 0.83em;
}
 .onekit-h2 {
    display: block;
    font-size: 1.5em;
    margin-block-start: 0.83em;
    margin-block-end: 0.83em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
}

.onekit-h3 {
    display: block;
    font-size: 1.17em;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
}

.onekit-h4 {
    display: block;
    margin-block-start: 1.33em;
    margin-block-end: 1.33em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
}

.onekit-h5 {
    display: block;
    font-size: 0.83em;
    margin-block-start: 1.67em;
    margin-block-end: 1.67em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
}

.onekit-h6 {
    display: block;
    font-size: 0.67em;
    margin-block-start: 2.33em;
    margin-block-end: 2.33em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
}

.onekit-hr {
    display: block;
    unicode-bidi: isolate;
    -webkit-margin-before: 0.5em;
    -webkit-margin-after: 0.5em;
    -webkit-margin-start: auto;
    -webkit-margin-end: auto;
    overflow: hidden;
    border-style: inset;
    border-width: 1px;
}

.onekit-iframe {
    border-width: 2px;
    border-style: inset;
    border-color: initial;
    border-image: initial;
}

.onekit-iframe {
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.onekit-input {
    writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0px;
    text-shadow: none;
    display: inline-block;
    text-align: start;
    appearance: textfield;
    background-color: -internal-light-dark(rgb(255, 255, 255), rgb(59, 59, 59));
    -webkit-rtl-ordering: logical;
    cursor: text;
    margin: 0em;
    font: 400 13.3333px Arial;
    padding: 1px 2px;
    border-width: 2px;
    border-style: inset;
    border-top-style: inset;
    border-right-style: inset;
    border-bottom-style: inset;
    border-left-style: inset;
    border-color: -internal-light-dark(rgb(118, 118, 118), rgb(195, 195, 195));
    border-image: initial;
}




.onekit-p {
    display: block;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
}

.onekit-label {
    cursor: default;
}

.onekit-u,
.onekit-ins {
    text-decoration: underline;

}

.onekit-legend {
    display: block;
    -webkit-padding-start: 2px;
    -webkit-padding-end: 2px;
    border-width: initial;
    border-style: none;
    border-color: initial;
    border-image: initial;
}

.onekit-pre,
.onekit-xmp,
.onekit-plaintext,
.onekit-listing {
    display: block;
    font-family: monospace;
    white-space: pre;
    margin: 1em 0;
}

.onekit-map {
    display: inline;
}

.onekit-mark {
    background-color: yellow;
    color: black;
}

