<video
class="onekit-image {{onekitClass}}"
style="width:{{weixin_width}};height:{{weixin_height}};{{onekitStyle}};"
src="{{src}}"
autoplay="{{autoplay}}"
controls="{{controls}}"
duration="{{duration}}"
loop="{{loop}}"
muted="{{muted}}"
poster="{{poster}}"

 
bindloadedmetadata="video_loadedmetadata"
bindtimeupdate="videoChange"
bindplay="video_play"
bindended="video_ended"
bindvtt="video_vtt"
>
<track id="track" />
<slot/>
</video>

