<view wx:if="{{type==''}}"
class="onekit-ol {{onekitClass}}"
style="counter-reset:section {{start}};list-style-type:none;{{onekitStyle}}"
id="{{onekitId}}"

bindtap="ol_tap"
>
<slot/>
</view>

